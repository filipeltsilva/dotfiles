filetype plugin indent on

set autoindent
set backspace=eol,indent,start
set copyindent
set history=1000
set hls
set ignorecase
set incsearch
set mouse=a
set nowrap
set number
set showcmd
set showmatch
set showmode
set smartcase
set title
set ttyfast
set ul=1000
set visualbell
set wildmenu
set wildmode=longest,list
